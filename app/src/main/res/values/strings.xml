<resources>
    <string name="app_name">Relatório de Terremotos</string>

    <!-- Texto padrão para mostrar com a localização do terremoto ("Perto do Pacific-Antarctic Ridge")
  se não houver distância específica de quilômetro do local primário [CHAR LIMIT=30] -->
    <string name="near_the">Perto de</string>

    <!-- Texto para mostrar na lista quando não houver terremotos [CHAR LIMIT=NONE] -->
    <string name="no_earthquakes">Nenhum terremoto encontrado.</string>

    <!-- Error message when there is no internet connectivity [CHAR LIMIT=NONE] -->
    <string name="no_internet_connection">Sem conexão com a internet.</string>

    <!-- Item de Menu de Configuração [CHAR LIMIT=NONE] -->
    <string name="settings_menu_item">Configurações</string>
    <!-- Título da SettingsActivity [CHAR LIMIT=NONE] -->

    <string name="settings_title">Configurações de Terremotos</string>

    <string name="title_activity_settings">SettingsActivity</string>

    <string name="settings_min_magnitude_label">Magnitude mínima</string>

    <string name="settings_min_magnitude_key" translatable="false">min_magnitude</string>

    <string name="settings_min_magnitude_default" translatable="false">0</string>

    <!-- Strings Para Preferência de Order-By [CHAR LIMIT=30] -->
    <string name="settings_order_by_label">Ordenar por</string>
    <string name="settings_order_by_key" translatable="false">order_by</string>
    <string name="settings_order_by_default" translatable="false">@string/settings_order_by_magnitude_value</string>

    <!-- Label para opção de order-by magnitude [CHAR LIMIT=20] -->
    <string name="settings_order_by_magnitude_label">Magnitude</string>
    <string name="settings_order_by_magnitude_value" translatable="false">magnitude</string>

    <!-- Label para order-by a opção mais recente [CHAR LIMIT=20] -->
    <string name="settings_order_by_most_recent_label">Mais recente</string>
    <string name="settings_order_by_most_recent_value" translatable="false">time</string>

</resources>
